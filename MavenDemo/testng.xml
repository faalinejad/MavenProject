<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestSUite" thread-count="5" parallel="methods">

	<test
		name="User should able to Login and Logout successfully in application"> <!-- Test Case -->
		<classes>
			<class name="QAGuru.MavenDemo.Class5Sept">
				<methods>
					<include name="doLogin" />
				</methods>
			</class>
		</classes>
	</test>

	<test
		name="User should able to Login and Logout successfully in application."> <!-- Test Case -->
		<classes>
			<class name="QAGuru.MavenDemo.Class5Sept">
				<methods>
					<include name="doLogin" />
				</methods>
			</class>
		</classes>
	</test>

